      "protocol": "/data/colour/default/x64"
    },
    {
      "type": "integer",
      "name": "navigation height (px)",
      "minimum": "50",
      "maximum": "100",
      "hint": "50, 100"
    },
    {
      "type": "abstraction",
      "name": "burger menu colour",
      "protocol": "/data/colour/default/x64"
    },
    {
      "type": "integer",
      "name": "menu list width",
      "minimum": "0",
      "maximum": "2000",
      "hint": "0, 2000"
    },
    {
      "type": "boolean",
      "name": "enable menu list height full page"
    },
    {
      "type": "abstraction",
